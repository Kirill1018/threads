<Window x:Class="threads.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:threads"
        mc:Ignorable="d"
        Title="потоки" Height="450" Width="800">
    <DockPanel>
        <UniformGrid Rows="1" DockPanel.Dock="Bottom">
            <Button Name="exit" Width="70" Height="30"></Button>
        </UniformGrid>
        <TextBlock Name="block" Margin="30, 70, 100, 120" Width="70"></TextBlock>
        <TextBlock Name="text_block" Margin="30, 70, 100, 120" Width="70"></TextBlock>
        <TextBlock Name="unit" Margin="30, 70, 100, 120" Width="70"></TextBlock>
        <TextBlock Name="module" Margin="30, 70, 100, 120" Width="70"></TextBlock>
    </DockPanel>
</Window>